{
  "name": "@tanstack-app/ui",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "description": "Shared UI component library",
  "exports": {
    ".": {
      "types": "./dist/react/react/index.d.ts",
      "import": "./src/react/index.ts"
    },
    "./react": {
      "types": "./dist/react/react/index.d.ts",
      "import": "./src/react/index.ts"
    },
    "./react/components/*": {
      "types": "./dist/react/react/components/*.d.ts",
      "import": "./src/react/components/*.tsx"
    },
    "./react/hooks/*": {
      "types": "./dist/react/react/hooks/*.d.ts",
      "import": "./src/react/hooks/*.ts"
    },
    "./react/lib/*": {
      "types": "./dist/react/react/lib/*.d.ts",
      "import": "./src/react/lib/*.ts"
    },
    "./solid": {
      "solid": "./src/solid/index.ts",
      "types": "./dist/solid/solid/index.d.ts",
      "default": "./src/solid/index.ts"
    },
    "./solid/components/*": {
      "solid": "./src/solid/components/*.tsx",
      "types": "./dist/solid/solid/components/*.d.ts",
      "default": "./src/solid/components/*.tsx"
    },
    "./solid/hooks/*": {
      "solid": "./src/solid/hooks/*.ts",
      "types": "./dist/solid/solid/hooks/*.d.ts",
      "default": "./src/solid/hooks/*.ts"
    },
    "./solid/lib/*": {
      "solid": "./src/solid/lib/*.ts",
      "types": "./dist/solid/solid/lib/*.d.ts",
      "default": "./src/solid/lib/*.ts"
    },
    "./shared": {
      "types": "./dist/react/shared/index.d.ts",
      "import": "./src/shared/index.ts"
    }
  },
  "scripts": {
    "build": "pnpm build:types",
    "build:types": "pnpm build:types:react && pnpm build:types:solid",
    "build:types:react": "tsc -p tsconfig.react.json",
    "build:types:solid": "tsc -p tsconfig.solid.json",
    "lint": "eslint .",
    "type-check": "pnpm type-check:react && pnpm type-check:solid",
    "type-check:react": "tsc -p tsconfig.react.json --noEmit",
    "type-check:solid": "tsc -p tsconfig.solid.json --noEmit",
    "error": "pnpm type-check && pnpm lint",
    "diagnostics": "pnpm type-check && pnpm lint",
    "storybook": "pnpm storybook:react",
    "storybook:react": "storybook dev -p 6006 --config-dir .storybook-react",
    "storybook:solid": "storybook dev -p 6007 --config-dir .storybook-solid",
    "build-storybook": "pnpm build-storybook:react && pnpm build-storybook:solid",
    "build-storybook:react": "storybook build --config-dir .storybook-react -o storybook-static-react",
    "build-storybook:solid": "storybook build --config-dir .storybook-solid -o storybook-static-solid"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.150.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "lucide-solid": "^0.562.0",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.15",
    "@storybook/react": "^8.6.15",
    "@storybook/react-vite": "^8.6.15",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "solid-js": "^1.9.10",
    "storybook": "^8.6.15",
    "storybook-solidjs": "1.0.0-beta.7",
    "storybook-solidjs-vite": "1.0.0-beta.7",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "solid-js": "^1.9.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "solid-js": {
      "optional": true
    }
  }
}